generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Class {
  id        String       @id @default(uuid())
  name      String
  students  Student[] @relation("ClassToStudent")
}

model Student {
  id        String       @id @default(uuid())
  name      String
  classes   Class[] @relation("ClassToStudent")
}
